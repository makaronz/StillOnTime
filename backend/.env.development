# Development Environment - Minimal Setup
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:3000

# Database (you can use a simple SQLite for development)
DATABASE_URL=postgresql://user:password@localhost:5432/stillontime_dev

# Redis (optional for development)
REDIS_URL=redis://localhost:6379

# JWT Secret (development only)
JWT_SECRET=development-jwt-secret-at-least-32-characters-long

# Google OAuth 2.0 (get from Google Cloud Console)
GOOGLE_CLIENT_ID=your-google-client-id-here
GOOGLE_CLIENT_SECRET=your-google-client-secret-here
GOOGLE_REDIRECT_URI=http://localhost:3000/auth/callback

# External APIs (optional for basic testing)
OPENWEATHER_API_KEY=your-openweather-api-key
GOOGLE_MAPS_API_KEY=your-google-maps-api-key