# Test Environment Configuration
NODE_ENV=test
PORT=3002
FRONTEND_URL=http://localhost:3000

# Database - Use test database
DATABASE_URL=postgresql://stillontime_user:stillontime_password@localhost:5432/stillontime_automation_test

# Redis - Use test Redis
REDIS_URL=redis://localhost:6379/1

