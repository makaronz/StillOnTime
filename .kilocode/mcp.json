{
  "mcpServers": {
    "postgres": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "postgresql://stillontime_user:stillontime_password@localhost:5432/stillontime_automation"
      ]
    },
    "redis": {
      "command": "uvx",
      "args": [
        "mcp-server-redis",
        "--url",
        "redis://localhost:6379"
      ]
    }
  }
}