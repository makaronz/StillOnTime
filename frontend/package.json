{
  "name": "stillontime-frontend",
  "version": "1.0.0",
  "description": "StillOnTime Film Schedule Automation System - Frontend Dashboard",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
